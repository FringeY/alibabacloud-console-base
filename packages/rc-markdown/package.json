    "@alicloud/console-toolkit-preset-component": "^1.2.9",
    "@alicloud/demo-rc-elements": "^1.1.22",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.14.4",
    "jest": "^26.6.3",
    "react": "^16.14.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "micromark": "^2.11.4",
    "micromark-extension-directive": "^1.2.0",
    "micromark-extension-gfm": "^0.3.3"
  },
  "scripts": {
    "start": "breezr start-storybook",
    "test": "breezr test:unit",
    "build:esm": "breezr build --engine babel --es-module",
    "build:cjs": "breezr build --engine babel",
    "build:bundle": "breezr build --engine webpack",
    "build:typings": "tsc --outDir build/types --declaration --emitDeclarationOnly",
